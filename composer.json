{
    "name": "juliendufresne/monolog-extension",
    "description": "Extends monolog with custom handlers",
    "require": {
        "php": "^7.1.3",
        "monolog/monolog": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "phpmetrics/phpmetrics": "^2.3",
        "phpstan/phpstan": "^0.9.2",
        "phpstan/phpstan-phpunit": "^0.9.4",
        "phpunit/phpunit": "^7.0",
        "sensiolabs/security-checker": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "JulienDufresne\\MonologExtension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JulienDufresne\\MonologExtension\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Dufresne",
            "email": "github@dfrsn.me"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    }
}
